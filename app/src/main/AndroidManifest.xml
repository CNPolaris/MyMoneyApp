<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymoneyapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:focusableInTouchMode="false"
        android:icon="@drawable/myicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/myicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddFamilyActivity"></activity>
        <activity android:name=".CreateFamilyActivity" />
        <activity android:name="com.familyManger.CreateFamilyActivity" />
        <activity android:name=".FamilyActivity" />
        <activity android:name=".ShowMonthData" />
        <activity android:name=".UpdateActivity" />
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".MyInfoActivity" />
        <activity android:name=".AddActivity" />
        <activity android:name=".chartActivity" />
        <activity android:name=".LoginInActivity" />
        <activity android:name=".LoginUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>